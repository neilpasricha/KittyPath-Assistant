    <div id="kittyContainer" class="container">

        <img src="\gui\kittyLogoTest.png" alt="kittyPath" width="500" height="181">
    </div>

    <div class="dropdown">
        <button id="searchButton" class="kitty-button">Search</button>
        <div id="myDropdownSearch" class="dropdown-contentSearch">
            
            <ul id="SearchTabs" class = "searchTab">
                <button id="CourseReq" class="tablink" ><font color = "white">Search by Requirement</font></button>
                <button id="CourseID" class="tablink" ><font color = "white">Search by Course ID/Course Number</font></button>
                <br>
            </ul>
            
    
            <div id="ClassById" class="SearchTabContent">
                <h2 id="SearchLabel" autocomplete="on" style="color:white;">Course Search:</h2>
                <form>
                    <input id="search-input" type="search" class="class_search" style="width:275px;height:30px;margin-top: 10px;" autocomplete="off" placeholder="Enter Class Name or Class Course Code...."> </input>
                </form>
                
                <button hidden id="clearSearch" class="class_search" position: "fixed">New Search</button>
                <select hidden size="10" id="options" class="chosen" style="border-radius: 5px; margin-top: 30px;"></select>
                <table hidden id="title" background="\gui\greyBG.png" style="width:500px;margin-top:70px">
                    <tr id="course" style="color:rgb(85, 241, 252); font-size:20px; margin-bottom: 0px">
                        <b></b>
                    </tr>
                </table>

                <table hidden id="tab" background="\gui\greyBG.png" style="width:500px;overflow-y: scroll;" border=1 frame=void rules=rows>
                    <tr>
                        <td style="color:white; font-size:18px">
                            <b>Number</b>
                        </td>
                        <td id="number" style="color:white; font-size:18px"></td>

                    </tr>
                    <tr>
                        <td style="color:white; font-size:18px">
                            <b>Section</b>
                        </td>
                        <td id="section" style="color:white; font-size:18px"></td>

                    </tr>
                    <tr>
                        <td style="color:white; font-size:18px">
                            <b>Times</b>
                        </td>
                        <td id="times" style="color:white; font-size:18px"></td>

                    </tr>
                    <tr>
                        <td style="color:white; font-size:18px">
                            <b>Room</b>
                        </td>
                        <td id="room" style="color:white; font-size:18px"></td>
                    </tr>
                    <tr>
                        <td style="color:white; font-size:18px">
                            <b>Instructor</b>
                        </td>
                        <td id="instructor" style="color:white; font-size:18px"></td>

                    </tr>
                    <tr>
                        <td style="color:white; font-size:18px">
                            <b>Date</b>
                        </td>
                        <td id="dates" style="color:white; font-size:18px"></td>

                    </tr>

                    <tr>

                        <td style="color:bisque;font-size:18px">
                            <b>Pre-requesites</b>
                        </td>
                        <td id="prerequesites" style="color:white; font-size:18px"></td>                       
                    </tr>

                    <tr>
                        <td style="color:bisque;font-size:18px">
                            <b></b>
                        </td>
                        <td class="infos">
                            <a id="details" style="color:rgb(141, 243, 155); font-size: 18px" href="#" target="_blank">Click to read more details...</a>
                        </td>
                    </tr>

                </table>
                <button hidden id="saveClassbtn" class="class_search">Save Course</button>
            </div>
        </div>
    </div>

    <div class="dropdown">
        <button id="planButton" class="kitty-button">Plan</button>
        <div id="myDropdownPlan" class="dropdown-contentPlan">
        <h2 id="SearchLabel" autocomplete="off" style="color:white; margin-left:10px">Please select a campus:</h2>
        <h3 id="SemesterSelectLabel" class="SemesterLabel" style="color: white; margin-left: 250px">Please select a semester to view:</h3>
        <h3 id="MajorSelectLabel" class="majorLabel" style="color: white; margin-left: 10px;">Please select a major</h3>
        
